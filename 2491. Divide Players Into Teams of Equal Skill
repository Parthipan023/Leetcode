class Solution {
    public long dividePlayers(int[] skill) 
    {
        Arrays.sort(skill);
        long sum=0;
        int len=skill.length;
        for(int i=0,j=len-1;i<j;i++,j--)
        {
            sum=sum+skill[i]*skill[j];
            if((skill[0]+skill[len-1])!=(skill[i]+skill[j]))
            {
                return -1;
            }
        }
        return sum;
        
    }
}
