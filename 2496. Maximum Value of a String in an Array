class Solution {
    public int maximumValue(String[] s) {
        int hig=0;
        int m=0;
        for(int i=0;i<s.length;i++)
        {
            m=Math.max(m,cnt(s[i]));
        }
        return m;
    }
        int cnt(String strs)
        {
            int count=0;
            int len=strs.length();
            for(int i=0;i<len;i++)
            {
                if(strs.charAt(i)>='0' && strs.charAt(i)<='9')
                    count++;
            }
            if(len==count)
            {
                int res=0;
                for(int i=0;i<len;i++)
                {
                    res*=10;
                    res+=strs.charAt(i)-'0';

                }
                return res;
            }
            return len;
    }
}
